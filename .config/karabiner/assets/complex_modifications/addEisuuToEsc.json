{
  "title": "Add eisuu to esc",
  "rules": [
    {
      "description": "Add eisuu to esc",
      "manipulators": [
        {
          "type": "basic",
          "from": {
            "key_code": "escape"
          },
          "to": [
            {
              "key_code": "japanese_eisuu"
            },
            {
              "key_code": "escape"
            }
          ],
          "conditions": [
            {
              "type": "frontmost_application_if",
              "bundle_identifiers": [
                "^com\\.apple\\.Terminal$"
              ]
            }
          ]
        }
      ]
    }
  ]
}

