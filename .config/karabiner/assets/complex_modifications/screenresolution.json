{
  "title": "Shortcuts to change screen resolution",
  "rules": [
    {
      "description": "Change screen resolution lower",
      "manipulators": [
        {
          "type": "basic",
          "from": {
            "key_code": "5",
            "modifiers": {
              "mandatory": ["control", "command", "option"]
            }
          },
          "to": [
            {
              "shell_command": "/usr/local/bin/screenresolution set 1280x800x32@0"
            }
          ]
        }
      ]
    },
    {
      "description": "Change screen resolution default",
      "manipulators": [
        {
          "type": "basic",
          "from": {
            "key_code": "6",
            "modifiers": {
              "mandatory": ["control", "command", "option"]
            }
          },
          "to": [
            {
              "shell_command": "/usr/local/bin/screenresolution set 1440x900x32@0"
            }
          ]
        }
      ]
    },
    {
      "description": "Change screen resolution to higher",
      "manipulators": [
        {
          "type": "basic",
          "from": {
            "key_code": "7",
            "modifiers": {
              "mandatory": ["control", "command", "option"]
            }
          },
          "to": [
            {
              "shell_command": "/usr/local/bin/screenresolution set 1680x1050x32@0"
            }
          ]
        }
      ]
    }
  ]
}
