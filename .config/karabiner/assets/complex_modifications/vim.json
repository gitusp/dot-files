{
  "title": "Add eisuu to esc-likes(for terminal like apps)",
  "rules": [
    {
      "description": "Add eisuu to escape",
      "manipulators": [
        {
          "type": "basic",
          "from": {
            "key_code": "escape"
          },
          "to": [
            {
              "key_code": "japanese_eisuu"
            },
            {
              "key_code": "escape"
            }
          ]
        }
      ]
    },
    {
      "description": "Add eisuu to c-[",
      "manipulators": [
        {
          "type": "basic",
          "from": {
            "key_code": "open_bracket",
            "modifiers": {
              "mandatory": ["control"]
            }
          },
          "to": [
            {
              "key_code": "japanese_eisuu"
            },
            {
              "key_code": "open_bracket",
              "modifiers": ["control"]
            }
          ]
        }
      ]
    }
  ]
}
