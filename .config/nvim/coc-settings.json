{
  "coc.preferences.colorSupport": false,
  "coc.preferences.formatOnSaveFiletypes": [
    "javascript",
    "javascriptreact",
    "typescript",
    "typescriptreact",
    "scss",
    "prisma"
  ],
  "list.normalMappings": {
    "v": "action:vsplit"
  },
  "stylelintplus.autoFixOnSave": true,
  "css.validate": false,
  "less.validate": false,
  "scss.validate": false,
  "wxss.validate": false,
  "eslint.autoFixOnSave": true,
  "prisma.prismaFmtBinPath": ""
}
